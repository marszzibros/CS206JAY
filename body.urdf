<robot name = "robot">
    <link name="Torso">
        <inertial>
            <origin xyz="0 0 1" rpy="0 0 0"/>
            <mass value="1" />
            <inertia ixx="100" ixy="0" ixz="0" iyy="100" iyz="0" izz="100" />
        </inertial>
        <visual>
            <origin xyz="0 0 1" rpy="0 0 0"/>
            <geometry>
                <box size="3 1.5 0.6" />
            </geometry>
            <material name="Cyan">
                <color rgba="0 1.0 1.0 1.0"/>
            </material>
        </visual>
        <collision>
            <origin xyz="0 0 1" rpy="0 0 0"/>
            <geometry>
                <box size="3 1.5 0.6" />
            </geometry>
        </collision>
    </link>
    <joint name="Torso_FrontLeftLeg" type="revolute">
       <parent link="Torso"/>
       <child  link="FrontLeftLeg"/>
       <origin rpy="0 0 0" xyz="-1 -1 0.7" />
       <axis xyz="0 1 0"/>
       <limit effort="0.0" lower="-3.14159" upper="3.14159" velocity="0.0"/>
    </joint>
    <link name="FrontLeftLeg">
        <inertial>
            <origin xyz="-1 -0.6 0.7" rpy="0 0 0"/>
            <mass value="1" />
            <inertia ixx="100" ixy="0" ixz="0" iyy="100" iyz="0" izz="100" />
        </inertial>
        <visual>
            <origin xyz="-1 -0.6 0.7" rpy="0 0 0"/>
            <geometry>
                <box size="0.2 0.2 0.7" />
            </geometry>
            <material name="Cyan">
                <color rgba="0 1.0 1.0 1.0"/>
            </material>
        </visual>
        <collision>
            <origin xyz="-1 -0.6 0.7" rpy="0 0 0"/>
            <geometry>
                <box size="0.2 0.2 0.7" />
            </geometry>
        </collision>
    </link>
</robot>